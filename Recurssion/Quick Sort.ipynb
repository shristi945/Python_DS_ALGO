    "def partition(a, si, ei):\n",
    "    count = 0 \n",
    "    p = a[si]\n",
    "    for i in a[si+1:]:\n",
    "        if i<p:\n",
    "            count += 1\n",
    "    index_i = si+count\n",
    "    temp = a[si]\n",
    "    a[si] = a[index_i]\n",
    "    a[index_i] = temp\n",
    "    \n",
    "    i = si\n",
    "    j = ei\n",
    "    \n",
    "    while i<j:\n",
    "        if a[i]<p:\n",
    "            i += 1\n",
    "        elif a[i]>p:\n",
    "            if a[j]>p:\n",
    "                j -= 1\n",
    "            elif a[j]<p:\n",
    "                temp = a[i]\n",
    "                a[i] = a[j]\n",
    "                a[j] = temp\n",
    "                i += 1\n",
    "                j -= 1\n",
    "            else:\n",
    "                break\n",
    "        else:\n",
    "            break\n",
    "                \n",
    "    return index_i\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "def quickSort(a, si, ei):\n",
    "    # Please add your code here\n",
    "    if si>=ei:\n",
    "        return\n",
    "    i = partition(a, si, ei)\n",
    "    qs(a, si, i-1)\n",
    "    qs(a, i+1, ei)\n",
    "\n",
    "n=int(input())\n",
    "arr=list(int(i) for i in input().strip().split(' '))\n",
    "quickSort(arr, 0, n)\n",
    "print(*arr)\n"
   ]
