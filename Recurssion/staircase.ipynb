    "n = int(input())\n",
    "def sc(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    if n>=3:\n",
    "        return sc(n-1)+sc(n-2)+sc(n-3)\n",
    "    \n",
    "    if n<3 and n>=2:\n",
    "        return sc(n-1)+sc(n-2)\n",
    "    \n",
    "    if n<2 and n>=1:\n",
    "        return sc(n-1)\n",
    "    \n",
    "print(sc(n))\n",
